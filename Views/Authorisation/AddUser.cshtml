@model Global_Intern.Models.User
@{
    ViewData["Title"] = "AddUser";
}

<h1>AddUser</h1>
<form action="ViewUser">
    <p>
        First Name:<input type="text" name="UserFirstName" />
    </p>
    <p>
        Last Name:<input type="text" name="UserLastName" />
    </p>
    <p>
        Email:<input type="text" name="UserEmail" />
    </p>
    <p>
        Password:<input type="password" name="UserPassword" />
    </p>
    <p>
        Home Country:<input type="text" name="UserHomeCountry" />
    </p>
    <p>
        Current Country:<input type="text" name="UserCurrentCountry" />
    </p>
    <p>
        Phone:<input type="text" name="UserPhone" />

    </p>
    <p>
        Gender:<input type="text" name="UserGender" />
    </p>
    <input type="submit" value="Register" />
</form>
